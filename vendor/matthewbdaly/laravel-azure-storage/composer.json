{
    "name": "matthewbdaly/laravel-azure-storage",
    "description": "Microsoft Azure Blob Storage integration for Laravel's Storage API",
    "type": "library",
    "keywords": ["storage","laravel","azure"],
    "require": {
        "league/flysystem-azure-blob-storage": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^9.3",
        "psy/psysh": "^0.11",
        "squizlabs/php_codesniffer": "^3.4",
        "vimeo/psalm": "^4.19"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\LaravelAzureStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matthewbdaly\\LaravelAzureStorage\\AzureStorageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }

}
